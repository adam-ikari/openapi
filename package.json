{
  "name": "openapi",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:tsp && npm run build:doc && npm run build:html",
    "build:tsp": "tsp compile src/index.tsp --output-dir output",
    "build:doc": "node scripts/build_doc.js",
    "build:html": "redocly build-docs output/schema/openapi.yaml --output=output/doc/html/openapi.html"
  },
  "peerDependencies": {
    "@azure-tools/typespec-ts": "latest",
    "@typespec/compiler": "latest",
    "@typespec/http": "latest",
    "@typespec/http-server-js": "latest",
    "@typespec/openapi": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/rest": "latest"
  },
  "devDependencies": {
    "@azure-tools/typespec-ts": "latest",
    "@redocly/cli": "^2.0.8",
    "@typespec/compiler": "^1.3.0",
    "@typespec/http": "latest",
    "@typespec/http-server-js": "latest",
    "@typespec/openapi": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/rest": "latest",
    "widdershins": "^4.0.1"
  },
  "private": true
}
